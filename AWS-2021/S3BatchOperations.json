{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"s3:PutObjectTagging",
				"s3:PutObjectVersionTagging"
			],
			"Resource": "arn:aws:s3:::bulk-operation-demo-yl/*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:GetObjectVersion",
				"s3:GetBucketLocation"
			],
			"Resource": "arn:aws:s3:::bulk-operation-demo-yl/*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"s3:PutObject",
				"s3:GetBucketLocation" 
			],
			"Resource": "arn:aws:s3:::bulk-operation-demo-yl/*"
		}
		// ,{
		// 	"Sid": "AllObjectActions",
		// 	"Effect": "Allow",
		// 	"Action": "s3:*Object",
		// 	"Resource": "arn:aws:s3:::bulk-operation-demo-yl/*"
		// }
	]
}